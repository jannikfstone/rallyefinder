    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "geolib": "^3.3.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
